{
  "expo": {
    "name": "CHE路NU",
    "slug": "chenu-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D8B26A"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.chenu.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "CHE路NU needs camera access for document scanning.",
        "NSPhotoLibraryUsageDescription": "CHE路NU needs photo library access for attachments.",
        "NSMicrophoneUsageDescription": "CHE路NU needs microphone access for voice messages."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#D8B26A"
      },
      "package": "io.chenu.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#D8B26A",
          "image": "./assets/splash.png",
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "chenu-mobile"
      },
      "apiUrl": "https://api.chenu.io",
      "wsUrl": "wss://api.chenu.io"
    },
    "owner": "chenu",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/chenu-mobile"
    }
  }
}
